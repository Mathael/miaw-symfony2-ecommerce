{% extends 'base.html.twig' %}

{% block body %}
    <h1>Buyorder</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ buyOrder.id }}</td>
            </tr>
            <tr>
                <th>Validatedate</th>
                <td>{% if buyOrder.validateDate %}{{ buyOrder.validateDate|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_buyorder_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('admin_buyorder_edit', { 'id': buyOrder.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
